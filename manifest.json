{
  "manifest_version": 2,
  "name": "Open-Private-Mode Extension",
  "version": "0.9",
  "description": "Open the current tab in private mode.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background":{
  "scripts": ["background.js"]
  },
  "permissions": [
	"tabs"  
  ]
}